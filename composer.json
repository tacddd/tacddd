{
    "name": "tacddd/tacddd",
    "description": "ProjectICKX Tactical DDD library",
    "type": "library",
    "homepage": "https://ickx.jp",
    "license": "MIT",
    "authors": [
        {
            "name": "akira wakaba",
            "homepage": "https://ickx.jp",
            "role": "lead developer"
        }
    ],
    "config": {
        "autoloader-suffix": "tacddd"
    },
    "autoload": {
        "psr-4": {
            "tacddd\\": "./src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "tacddd\\tests\\": "./tests/"
        }
    },
    "keywords": [
        "tacddd"
    ],
    "require": {
        "php": ">=8.2.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~10.0.0",
        "friendsofphp/php-cs-fixer": "~3.21.0"
    }
}